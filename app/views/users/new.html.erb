<div id="banner">
	<div class="hero banner_1">
		<div class="container-headers">
			<div class="alldock-logo"></div>
			<p class="large brandon">Ultra performant,</p>
			<p class="large brandon">Ultra élégant</p>
		</div>
	</div>
</div>
<div class="form-wrap  <%= "ended" if Rails.application.config.ended %>">
    <% if Rails.application.config.ended %>
        <p class="thanks brandon">MERCI D'AVOIR PARTICIPÉ AU LANCEMENT</p>
        <p class="compiling brandon">NOUS COMPILONS LES RÉ ULTATS. UN EMAIL SERA ENVOYÉ À TOUS LES PARTICIPANTS AVEC LEUR CODE COUPON DANS LES PROCHAINS JOURS</p>
        <p class="go-to brandon">En attandant, visitez notre détaillant: <a href="http://fr.mobiessentials.com/all-dock">www.mobiessentials.com</a></p>
        <p class="again brandon">Merci à tous!</p>
    <% else %>
        <p class="byline brandon">Inscrivez-vous pour en apprendre d'avantage</p>
        <%= form_for @user, :url => { :action => "create" } do |f| %>
            <%= f.text_field :email, :placeholder => "Enter Email", :class => "email brandon" %>
            <input class="submit brandon" name="commit" type="submit" value="En voir plus" />
        <% end %>
    <% end %>
</div>